{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "preserveConstEnums": true
  },
  "exclude": [
    "logs",
    "node_modules"
  ]
}
